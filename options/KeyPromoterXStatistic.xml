<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'F3'</ShortCut>
          <Description>Declaration</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project Structure...">
        <Item>
          <IdeaActionID>ShowProjectStructureSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+S'</ShortCut>
          <Description>Project Structure...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Push...">
        <Item>
          <IdeaActionID>Vcs.Push</IdeaActionID>
          <ShortCut>'Ctrl+Shift+K'</ShortCut>
          <Description>Push...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'ProxyTest.main()'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F10'</ShortCut>
          <Description>Run 'ProxyTest.main()'</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'ProxyTest'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'ProxyTest'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'SpringbootHelloApplication (1)'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'SpringbootHelloApplication (1)'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>